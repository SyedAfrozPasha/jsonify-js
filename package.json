{
  "name": "jsonify-js",
  "version": "0.6.0",
  "description": "A simple and light-weight JSON handler with promise support",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/app.js",
    "start:dev": "nodemon --exec babel-node ./src/index.js",
    "clean": "node buildInit.js && mkdir dist",
    "copy": "node copyToDist.js",
    "build-babel": "babel -d ./dist ./src/* -s",
    "build": "npm run clean && npm run build-babel && npm run copy",
    "format": "npx prettier --write ./src/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyedAfrozPasha/jsonify-js.git"
  },
  "keywords": [
    "json",
    "parse",
    "stringify",
    "promise",
    "async",
    "await",
    "logger",
    "file",
    "error",
    "handler",
    "format"
  ],
  "author": "Syed Afroz Pasha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SyedAfrozPasha/jsonify-js/issues"
  },
  "homepage": "https://github.com/SyedAfrozPasha/jsonify-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "prettier": "2.3.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3"
  }
}
